<html xmlns:c="http://java.sun.com/jsp/jstl/core"  
xmlns:spring="http://www.springframework.org/tags" 
xmlns:jsp="http://java.sun.com/JSP/Page">
	<jsp:directive.attribute name="title" required="true" rtexprvalue="true" description="Title for the page"/> 
	<head>
		<meta charset="utf-8"/>
		<title><c:out value="${title}"/></title>
		<spring:url value="/resources/styles" var="styleUrl" />
		<link href="${styleUrl}/main.css" rel="stylesheet"/>
	</head>
	
	<body>
            <jsp:include page="/WEB-INF/view/jsp/nav.jspx"/>	
                    <jsp:doBody />
            <jsp:include page="/WEB-INF/view/jsp/footer.jspx"/>	
	</body>
</html>